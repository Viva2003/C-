#include<iostream>
using namespace std;
void max(int x,int y){
    if(x>y){
        cout<<x<<" is highest num"<<endl;
    }
    else if(x==y){
        cout<<"Both x and y are equal"<<endl;
    }
    else{
        cout<<y<<" is highest num"<<endl;
    }
}
int main(){
    int x,y;
    cout<<"Enter x:"<<endl;
    cin>>x;
    cout<<"Enter y:"<<endl;
    cin>>y;
    max(x,y);
    return 0;
    
}
